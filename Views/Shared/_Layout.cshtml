<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>BHMS - @ViewData["Title"]</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/lib/md5/css/mdb.min.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/BHMS.styles.css" asp-append-version="true" />
    <link rel='stylesheet' href='https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css' integrity='sha512-DTOQO9RWCH3ppGqcWaEA1BIZOC6xxalwEsw9c2QQeAIftl+Vegovlnee1c9QX4TctnWMn13TZye+giMm8e2LwA==' crossorigin='anonymous'/>
</head>
<body>
    @Html.Partial("_Header")
   <div>
        <div class="row">
            <!-- Main Content Area -->
            <div class="col-md-8">
                <main role="main">
                    <!-- Content from views will be rendered here -->
                    @RenderBody()
                </main>
            </div>
            <div class="col-md-4">
                <div class="container">
                   
                </div>
            </div>
        </div>
    </div>

    @* @Html.Partial("_Footer") *@
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/lib/md5/js/mdb.es.min.js"></script>
    <script src="~/lib/md5/js/mdb.umd.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/js/communication.js"></script>
    <script src="~/js/reports.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
